{
  "name": "@folio-eis/bf-folio-wrapper",
  "version": "1.2.0",
  "description": "Linked Data Editor - Folio Wrapper",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.20.1"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-marva ./translations/ui-marva/compiled",
    "lint": "eslint .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.9.0",
    "@folio/eslint-config-stripes": "^6.0.0",
    "@folio/stripes": "^9.0.0",
    "@folio/stripes-cli": "^3.1.0",
    "@folio/stripes-core": "^10.1.1",
    "@formatjs/cli": "^4.2.29",
    "@jest/globals": "^26.6.2",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.10",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "eslint": "^7.32.0",
    "inflected": "^2.0.4",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-junit": "^12.0.0",
    "mocha": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "@folio-eis/marva-next": "^1.0.1"
  },
  "resolutions": {
    "favicons-webpack-plugin": "6.0.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^9.0.0",
    "react": "*",
    "react-intl": "^6.4.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.0.1"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "Linked data editor",
    "route": "/linked-data-editor",
    "home": "/linked-data-editor/search",
    "icons": [
      {
        "name": "app",
        "alt": "",
        "title": "Linked data editor"
      }
    ],
    "okapiInterfaces": {},
    "permissionSets": [
      {
        "permissionName": "module.bf-folio-wrapper.enabled",
        "displayName": "Linked Data: Module is enabled",
        "visible": true,
        "subPermissions": []
      },
      {
        "permissionName": "ui-linked-data.all",
        "displayName": "Linked Data: All Permissions",
        "visible": true,
        "subPermissions": [
          "module.bf-folio-wrapper.enabled",
          "linked-data.resources.bib.get",
          "linked-data.resources.bib.post",
          "linked-data.resources.bib.put",
          "linked-data.resources.bib.delete",
          "linked-data.resources.bib.marc.get",
          "linked-data.resources.reindex.post",
          "linked-data.resources.graph.get",
          "linked-data.resources.bib.id.get",
          "linked-data.profiles.get",
          "search.linked-data.work.collection.get",
          "search.linked-data.authority.collection.get"
        ]
      },
      {
        "permissionName": "settings.bf-folio-wrapper.enabled",
        "displayName": "Settings (Linked Data): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ]
      }
    ]
  }
}
